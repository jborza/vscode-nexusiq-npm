{
  "name": "vscode-nexusiq-npm",
  "displayName": "Nexus IQ for NPM",
  "description": "Unofficial Nexus IQ extension for NPM projects",
  "version": "0.0.1",
  "publisher": "jborza",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nexusiq.npm.evaluate"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "nexusiq.npm.evaluate",
        "title": "Nexus IQ Evaluate"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Nexus IQ configuration",
      "properties": {
        "nexusiq.npm.url": {
          "type": "string",
          "default": "http://127.0.0.1:8070",
          "description": "URL of the Nexus IQ server"
        },
        "nexusiq.npm.username": {
          "type": "string",
          "default": "admin",
          "description": "Your Nexus IQ user name"
        },
        "nexusiq.npm.password": {
          "type": "string",
          "default": "admin123",
          "description": "Your Nexus IQ password"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/lodash": "^4.14.73",
    "jsonc-parser": "^1.0.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "pluralize": "^7.0.0",
    "request": "^2.81.0",
    "snyk": "^1.189.0"
  },
  "snyk": true
}
